<resources>

    <!-- Gestures' key codes. If the gesture is not supported set it to 0. -->
    <integer name="config_doubleTapKeyCode">143</integer>

    <!-- Default settings for gestures:
             0 - Disabled
             1 - Wake
             2 - Pulse
             3 - Flashlight
             4 - Airplane
             5 - Play/Pause
             6 - Next
             7 - Previous
             8 - Camera
             9 - Dialer -->
    <integer name="config_doubleTapDefault">1</integer>

    <!-- Whether to intercept inputs on screen off. -->
    <bool name="config_enableKeyHandler">true</bool>

    <!-- Path to fast charging status file to detect whether an oem fast charger is active -->
    <string name="config_oemFastChargerStatusPath" translatable="false">/sys/class/power_supply/bms/fastcharge_mode</string>

    <!-- Is the battery LED intrusive? Used to decide if there should be a disable option -->
    <bool name="config_intrusiveBatteryLed">true</bool>

    <!-- Device model (Build.MODEL) to spoof for netflix, if not empty -->
    <string name="config_netflixSpoofModel" translatable="false">M2101K9G</string>

    <!-- Pocket Lock -->
    <bool name="config_pocketModeSupported">true</bool>

</resources>
